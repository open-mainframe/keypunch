{
  "name": "electron-webpack-quick-start",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "compile": "electron-webpack",
    "dev": "electron-webpack dev",
    "dist": "npm run compile && electron-builder",
    "dist:dir": "npm run dist --dir -c.compression=store -c.mac.identity=null",
    "format": "prettier --write 'src/**/*.{js,jsx}'"
  },
  "dependencies": {
    "ace": "^1.3.0",
    "bluebird": "^3.4.7",
    "brace": "^0.9.1",
    "font-awesome": "^4.7.0",
    "jsftp": "^1.5.5",
    "promise-ftp": "^1.3.2",
    "react": "^15.4.2",
    "react-ace": "^4.1.0",
    "react-desktop": "^0.3.5",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.7",
    "react-treebeard": "^1.1.4",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "source-map-support": "^0.4.10"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.0.0-beta.47",
    "electron": "2.0.0",
    "electron-builder": "^20.11.1",
    "electron-webpack": "^2.0.1",
    "prettier": "1.12.1",
    "redux-logger": "^3.0.6",
    "webpack": "^4.8.1"
  },
  "resolutions": {
    "webpack-sources": "1.0.1"
  },
  "electronWebpack": {
    "title": "Keypunch"
  },
  "prettier": {
    "bracketSpacing": true,
    "jsxBracketSameLine": false,
    "printWidth": 80,
    "semi": true,
    "singleQuote": false,
    "tabWidth": 2,
    "trailingComma": "none",
    "useTabs": false
  }
}
